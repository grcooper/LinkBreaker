{
  "manifest_version": 2,
  "name": "LinkBreaker",
  "version": "1.0.1",
  "description": "Break those pesky links!",
  "browser_action": {
    "default_icon": {                    
      "19": "images/linkbreaker.png"            
    },
    "default_title": "LinkBreaker"      
  },
  "author": "Daniel and Graham",
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["jquery-2.1.4.min.js","BlockGeneration.js"]
    }
  ],
  "background" : {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs"]
}